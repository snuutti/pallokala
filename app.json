{
    "expo": {
        "name": "Pallokala for PufferPanel",
        "slug": "pallokala",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "pallokala",
        "userInterfaceStyle": "automatic",
        "jsEngine": "hermes",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "config": {
                "usesNonExemptEncryption": false
            },
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "io.github.snuutti.pallokala"
        },
        "androidNavigationBar": {
            "barStyle": "dark-content",
            "backgroundColor": "#ffffff"
        },
        "plugins": [
            "expo-router",
            "expo-font",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
